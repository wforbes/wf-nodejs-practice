{
	"compilerOptions": {
		"resolveJsonModule": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"baseUrl": ".",
		"target": "ES6",
		"module": "CommonJS",
		"allowJs": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"removeComments": true,
		"noImplicitAny": false,
		"outDir": "dist"
	}
}